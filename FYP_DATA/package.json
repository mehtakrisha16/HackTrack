{
  "name": "hacktrack-mumbai-backend",
  "version": "1.0.0",
  "description": "Backend API server for HackTrack Mumbai - Real-time authentication and database management",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "start:clean": "taskkill /IM node.exe /F >nul 2>&1 & node src/server.js",
    "kill:port": "netstat -ano | findstr :5000 && for /f \"tokens=5\" %a in ('netstat -ano ^| findstr :5000') do taskkill /PID %a /F",
    "test": "jest",
    "seed": "node scripts/seedDatabase.js",
    "db:setup": "node scripts/seedDatabase.js",
    "db:reset": "node scripts/seedDatabase.js --reset",
    "db:test": "node scripts/test-atlas.js",
    "db:template": "node scripts/test-atlas.js --template",
    "scrape": "node src/services/opportunityScraper.js",
    "scrape:dev": "nodemon src/services/opportunityScraper.js",
    "scrape:standalone": "node src/services/standaloneScraper.js",
    "scrape:service": "node src/services/standaloneScraper.js"
  },
  "keywords": [
    "hacktrack",
    "mumbai",
    "express",
    "mongodb",
    "authentication",
    "api"
  ],
  "author": "HackTrack Mumbai Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "google-auth-library": "^10.3.0",
    "googleapis": "^160.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "puppeteer": "^21.5.2",
    "cheerio": "^1.0.0-rc.12",
    "node-cron": "^3.0.3",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
